<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- JAVA와 연결할 Mapper 파일 설정 -->
<mapper namespace="poly.persistance.mapper.IBoardMapper">

 <!-- 게시판 리스트 쿼리 -->
	<select id="getBoardList" resultType="NoticeDTO">
	SELECT * FROM INOTICE
	</select>
<!-- 게시판 선택  -->	
	<select id="searchList" parameterType="NoticeDTO" resultType="NoticeDTO">
	SELECT * FROM INOTICE WHERE NOTIC_NO=#{notice_no}
	</select>
	
		<delete id="getDeletePost"  parameterType="BoardDto">
	DELETE FROM BOARD WHERE POST_NO=#{post_no}
	</delete>
 
</mapper>